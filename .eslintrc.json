{
  "env": {
    "node": true,
    "commonjs": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": "error",
    "max-depth": ["error", 2],
    "max-lines-per-function": ["error", 10]
  }
}
